@model ManageUsersViewModel
@{
    ViewData["Title"] = "Manage users";
}

<h2>@ViewData["Title"]</h2>

<h3>Admins</h3>

<table class="table table-bordered table-hover">
    <thead>
        <tr>
            <td>Id</td>
            <td>E-mail</td>
        </tr>
    </thead>
    <tbody>
        @foreach(var user in Model.Admins)
        {
            <tr>
                <td>@user.Id</td>
                <td>@user.Email</td>
            </tr>
        }
    </tbody>
</table>

<h3>Everyone</h3>

<table class="table table-bordered table-hover">
    <thead>
        <tr>
            <td>Id</td>
            <td>E-mail</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model.Everyone)
        {
            <tr>
                <td>@user.Id</td>
                <td>@user.Email</td>
            </tr>
        }
    </tbody>
</table>
